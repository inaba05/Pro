<h1>Machines#new</h1>
<p>Find me in app/views/machines/new.html.erb</p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <%= form_for(@machine) do |f| %>
          <%= f.label :number %>
          <%= f.text_field :number, class: 'form-control' %>
          <%= f.label :model_id %>
          <%= f.collection_select :model_id, Model.all, :id, :name,:prompt => "選択してください" %>
          <%= f.label :from %>
          <%= f.text_field :from, class: 'form-control' %>
          <%= f.label :to %>
          <%= f.text_field :to, class: 'form-control' %>
          <%= f.submit "Create my account", class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
<%= button_to "戻る", machines_index_path ,method: :get %>
