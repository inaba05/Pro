<h1>Machines#edit</h1>
<p>Find me in app/views/machines/edit.html.erb</p>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@machine) do |f| %>
      <%= f.label :number %>
      <%= f.text_field :number, class: 'form-control' %>
      <%= f.label :model_id %>
      <%= f.text_field :model_id, class: 'form-control' %>
      <%= f.label :from %>
      <%= f.text_field :from, class: 'form-control' %>
      <%= f.label :to %>
      <%= f.text_field :to, class: 'form-control' %>
      <%= f.submit "変更", class: "btn btn-primary" %>
    <% end %>
</div>
<%= button_to "戻る", machines_index_path ,method: :get %>
