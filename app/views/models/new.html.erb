<h1>Models#new</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@model) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.submit "作成", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
<%= button_to "戻る", models_index_path ,method: :get %>
